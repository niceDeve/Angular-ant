{
  "productName": "ant-downloader",
  "appId": "com.electron.ant_downloader",
  "directories": {
    "output": "./release/"
  },
    "files": [
        "**/*",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json",
        "!backend/"
    ],
  "win": {
    "icon": "logos/icon.ico",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "logos",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "category": "public.app-category.p2p",
    "icon": "logos",
    "target": [
      "AppImage"
    ]
  }
}
